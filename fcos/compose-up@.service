[Unit]
Description=Run compose up for %I
Wants=network-online.target compose-build@%i.service
After=network-online.target compose-build@%i.service
ConditionPathExists=/etc/compose/build/%i/compose.yml

[Service]
WorkingDirectory=/etc/compose/build/%i
ExecStart=/usr/bin/docker-compose -f compose.yml up

[Install]
WantedBy=multi-user.target
